{
  "manifest_version" : 2,

  "name" : "TabWatch",
  "description" : "This extension will warn you if you have too many tabs open",
  "version" : "0.2.0",

  "background" : {
    "scripts" : ["main.js"],
    "persistent" : true
  },

  "browser_action" : {
    "default_icon" : "icon.png",
    "default_popup" : "dialog.html"
  },

  "permissions" : [
    "tabs"
  ]
}
