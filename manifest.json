{
  "manifest_version" : 2,

  "name" : "TabWatch",
  "description" : "This extension will warn you if you have too many tabs open",
  "version" : "0.5.1",
  "icons": {
    "16": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "background" : {
    "scripts" : ["background.js"],
    "persistent" : true
  },

  "browser_action" : {
    "default_title" : "TabWatch",
    "default_icon" : "icon19.png",
    "default_popup" : "dialog.html"
  },

  "minimum_chrome_version" : "51.0",

  "permissions" : [
    "tabs",
    "storage"
  ]
}
